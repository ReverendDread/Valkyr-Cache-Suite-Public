<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="suite.dialogue.DualInputDialogue">
   <children>
      <HBox spacing="10.0">
         <children>
            <TextField fx:id="min" promptText="Minimum" />
            <TextField fx:id="max" promptText="Maximum" />
            <Button mnemonicParsing="false" onAction="#finish" text="Finish" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
