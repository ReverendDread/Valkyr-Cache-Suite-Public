<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@modena_dark.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox>
               <children>
                  <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                  <TextField fx:id="search" promptText="Search an id..." />
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <HBox spacing="10.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                     <children>
                        <Canvas fx:id="canvas" height="128.0" width="128.0" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets bottom="25.0" />
                           </HBox.margin>
                        </Canvas>
                     </children>
                  </HBox>
                  <HBox spacing="10.0" VBox.vgrow="ALWAYS" />
                  <GridPane vgap="10.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="65.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.0" minWidth="10.0" prefWidth="135.0" />
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="50.0" />
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="135.0" />
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="50.0" />
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="238.0" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Speed:">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </Label>
                        <TextField fx:id="speed" GridPane.columnIndex="1" />
                        <Label text="Direction:" GridPane.rowIndex="1">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </Label>
                        <TextField fx:id="direction" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label text="Sprites:" GridPane.rowIndex="2">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </Label>
                        <TextField fx:id="spriteIds" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Button mnemonicParsing="false" onAction="#save" text="Save" GridPane.columnIndex="6" GridPane.rowIndex="2" />
                        <Button fx:id="setSpeed" mnemonicParsing="false" text="Set" GridPane.columnIndex="2" />
                        <Button fx:id="setDirection" mnemonicParsing="false" text="Set" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Button fx:id="setSprites" mnemonicParsing="false" text="Set" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <TextField fx:id="textureType" GridPane.columnIndex="4" />
                        <TextField fx:id="aField1585" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                        <TextField fx:id="aField1591" GridPane.columnIndex="4" GridPane.rowIndex="2" />
                        <Label text="Texture Types:" GridPane.columnIndex="3" />
                        <Label text="field1585:" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <Label text="field1591:" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <Button fx:id="setTextureTypes" mnemonicParsing="false" text="Set" GridPane.columnIndex="5" />
                        <Button fx:id="setField1585" mnemonicParsing="false" text="Set" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                        <Button fx:id="setField1591" mnemonicParsing="false" text="Set" GridPane.columnIndex="5" GridPane.rowIndex="2" />
                     </children>
                  </GridPane>
               </children>
               <padding>
                  <Insets bottom="5.0" right="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
